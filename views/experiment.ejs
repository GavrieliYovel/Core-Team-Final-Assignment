<a href="#model-successes" id="s-model" class="trigger-btn" data-toggle="modal" hidden>Click to Open Confirm Modal</a>
<!-- Modal successes -->
<div id="model-successes" class="modal fade">
    <div class="modal-dialog modal-confirm">
        <div class="modal-content">
            <div class="modal-header">
                <div class="icon-box">
                    <i class="fa-regular fa-check"></i>
                </div>
            </div>
            <div class="modal-body row d-flex justify-content-center">
