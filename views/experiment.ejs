            </div>
            <div class="modal-footer">
                <button class="btn btn-success btn-block" data-dismiss="modal">OK</button>
            </div>
        </div>
    </div>
</div>

<a href="#model-failed" id="f-model" class="trigger-btn" data-toggle="modal" hidden>Click to Open Confirm Modal</a>
<!-- Modal failed -->
<div id="model-failed" class="modal fade">
    <div class="modal-dialog modal-confirm failed-model">
        <div class="modal-content">
            <div class="modal-header">
                <div class="icon-box failed-model">
                    <i class="fa-regular fa-x"></i>
                </div>
            </div>
            <div class="modal-body row d-flex justify-content-center">
                <img src="images/fail.png">
